{
	"name": "summit2021-fall",
	"version": "0.0.1",
	"scripts": {
		"dev": "env-cmd svelte-kit dev",
		"build": "env-cmd svelte-kit build",
		"preview": "svelte-kit preview",
		"publish": "env-cmd wrangler publish",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^1.0.0-next.2",
		"@sveltejs/adapter-cloudflare-workers": "next",
		"@sveltejs/kit": "next",
		"@svitejs/svelte-preprocess-svg": "^1.0.1",
		"@types/cookie": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rollup-plugin-hoist-import-deps": "^1.1.0",
		"svelte": "^3.34.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite-imagetools": "^4.0.1"
	},
	"type": "module",
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.4.1",
		"env-cmd": "^10.1.0",
		"minireset.css": "^0.0.7"
	}
}
